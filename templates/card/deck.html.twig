{% extends "base.html.twig" %}

{% block title %}Deck{% endblock %}

{% block content %}
<h1 class="arubrik">Deck of Cards</h1>
<div class="content">
    {% for card in card_deck %}
        <img class="cards" src="{{ asset("../img/cards/" ~ card ~ ".png") }}" width="100" height="140" alt="{{card}}">
    {% endfor %}
</div>

{% endblock %}
